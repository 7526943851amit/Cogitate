$(document).ready(function() {

    $('#learn_more_after').click(function(e) {
        e.preventDefault(); 

        var target = $(this).attr('href'); 

        $('html, body').scrollTop($(target).offset().top);
    });
});



$(document).ready(function() {

    $('a[href="#"]').attr('href', 'javascript:void(0)');
});
